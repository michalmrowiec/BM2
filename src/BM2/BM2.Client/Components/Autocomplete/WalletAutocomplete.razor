@inherits AutocompleteBase<BM2.Shared.DTOs.WalletDTO>

@{
    base.BuildRenderTree(__builder);
}

@code {

    protected override void OnInitialized()
    {
        Uri = "api/v1/wallets";
        SearchFunc = (entity, value) => entity.WalletName.Contains(value, StringComparison.InvariantCultureIgnoreCase);
        OrderFunc = x => x.WalletName;
        base.OnInitialized();
    }

}